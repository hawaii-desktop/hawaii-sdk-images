{
    "runtime": "org.freedesktop.Platform",
    "metadata": "metadata.sdk",
    "cleanup": [
        "/cache",
        "/man",
        "/share/doc",
        "/share/man",
        "/share/gtk-doc",
        "/share/devhelp",
        "/lib/systemd",
        "*.la",
        "*.a"
    ],
    "sdk-extensions": [
        "org.freedesktop.Sdk.Debug",
        "org.freedesktop.Sdk.Locale"
    ],
    "sdk": "org.freedesktop.Sdk",
    "build-runtime": true,
    "runtime-version": "master",
    "cleanup-commands": [
        "touch -d @0 /usr/share/fonts",
        "touch -d @0 /usr/share/fonts/*",
        "fc-cache -fs"
    ],
    "writable-sdk": true,
    "build-options": {
        "arch": {
            "x86_64": {
                "cxxflags": "-O3 -g",
                "cflags": "-O3 -g"
            }
        },
        "cxxflags": "-O2 -g",
        "env": {
            "V": "1"
        },
        "cflags": "-O2 -g"
    },
    "cleanup-platform": [
        "/include",
        "/share/aclocal",
        "/share/pkgconfig",
        "/lib/pkgconfig",
        "/share/gir-1.0",
        "/share/vala"
    ],
    "id-platform": "org.hawaiios.Platform",
    "metadata-platform": "metadata.platform",
    "platform-extensions": [
        "org.freedesktop.Platform.Locale"
    ],
    "modules": [
